{
    "name": "jdavidbakr/address-verification",
    "description": "A service that performs address verification using the API at https://www.intelligentsearch.com/",
    "license": "MIT",
    "keywords": [
        "Address Verification",
        "intelligentsearch"
    ],
    "require": {
        "guzzlehttp/guzzle": "~5.3|~6.0.1|~6.1"
    },
    "authors": [
        {
            "name": "Jon Baker",
            "email": "me@jdavidbaker.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "jdavidbakr\\AddressVerification\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "jdavidbakr\\AddressVerification\\AddressVerificationServiceProvider"
            ],
            "aliases": {
                "AddressVerification": "jdavidbakr\\AddressVerification\\Facades\\AddressVerificationFacade"
            }
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3",
        "orchestra/testbench": "^3.8",
        "spatie/array-to-xml": "^2.11"
    }
}
